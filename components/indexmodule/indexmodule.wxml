<view class="modle_group">
  <view class="modle_title">
    <view class="modle-movetitle">{{title}}</view>
    <navigator class="more" url="{{moreurl}}">更多</navigator>
  </view>
  <scroll-view class="module_scroll" scroll-x="{{true}}">
    <itemview wx:for="{{items}}" wx:key="{{items.title}}" item = "{{item}}" itemurl="/pages/detail/detail?type={{type}}&id={{item.id}}"></itemview>
  </scroll-view>
</view>
