.item_navigator {
  width: 200rpx;
  margin-right: 20rpx;
  display: inline-block;
}

.item_navigator .item-group {
  width: 100%;
}

.item-group .item-img {
  width: 100%;
  height: 284rpx;
}

.item-img .thumbnail-img {
  width: 100%;
  height: 100%;
}

.item-group .item-title {
  font-size: 12px;
  text-align: center;
  margin-top: 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-bottom: 20rpx;
}
